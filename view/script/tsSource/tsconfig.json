{
   "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "isolatedModules": false,
      "jsx": "react",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "declaration": false,
      "noImplicitAny": false,
      "noImplicitUseStrict": false,
      "removeComments": true,
      "noLib": false,
      "preserveConstEnums": true,
      "suppressImplicitAnyIndexErrors": true,
      "outDir": "../"
   },
   "exclude": [
      "node_modules",
      "Types.ts"
   ],
   "compileOnSave": true,
   "buildOnSave": false,
   "atom": {
      "rewriteTsconfig": false
   }
}
